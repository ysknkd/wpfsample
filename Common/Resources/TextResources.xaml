    <system:String x:Key="MSG_DIALOG_TITLE_CONFIRM">確認</system:String>
    <system:String x:Key="MSG_DIALOG_TITLE_ERROR">エラー</system:String>
    <system:String x:Key="MSG_DIALOG_REQUIRE">必須項目が入力されていません</system:String>
    <system:String x:Key="MSG_DIALOG_ADDED">を追加しました</system:String>
    
    <system:String x:Key="MSG_ERROR_ARTIST_ALPHANUMERIC">英数字で入力してください</system:String>
