<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Common"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!-- ボタン -->
    <system:String x:Key="LABEL_SEARCH">検索</system:String>
    <system:String x:Key="LABEL_ADD">追加</system:String>
    <system:String x:Key="LABEL_ADDTAB">タブを追加</system:String>
    <system:String x:Key="LABEL_BUTTON_DUMMY">ダミーボタン(F*)</system:String>
   
    <!-- ラベル -->
    <system:String x:Key="LABEL_TAB_TEST">タブのテスト</system:String>
    <system:String x:Key="LABEL_ARTIST_TITLE">アーティスト</system:String>
    <system:String x:Key="LABEL_ARTISTS">アーティスト名</system:String>
    <system:String x:Key="LABEL_TEST_OF_COMBOBOX">コンボボックスのテスト</system:String>
    <system:String x:Key="LABEL_DUMMY">ダミーのラベル</system:String>
    
